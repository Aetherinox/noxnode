{
  "name": "noxnode",
  "version": "1.0.0",
  "description": "Nox NodeJS template",
  "main": "index.js",
  "author": "Aetherinox <adminservx@proton.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aetherinox/noxnode.git"
  },
  "contributors": [
      {
          "name": "Aetherinox",
          "email": "adminservx@proton.me",
          "url": "https://github.com/Aetherinox"
      },
      {
          "name": "AdminServ",
          "email": "adminservx@proton.me",
